main_data_dir = "UCI HAR Dataset"
testdir ="test"
traindir ="train"
subject_test = "subject_test.txt"
x_test = "X_test.txt"
y_test = "y_test.txt"
subject_train = "subject_train.txt"
x_train = "X_train.txt"
y_train = "y_train.txt"
features = "features.txt"
headerslist <- vector(mode = "character", length = 0)
headersindices <-vector(mode = "numeric", length = 0)
initDataset <- function()
{
if(!file.exists(main_data_dir))
{
print(paste("data file : not found",main_data_dir))
return
}
setwd(main_data_dir)
setwd(testdir)
xtest <- read.table(x_test)
ytest <- read.table(y_test)
print(ncol(xtest))
setwd("../")
setwd(traindir)
xtrain <- read.table(x_train)
ytrain <- read.table(y_train)
print(ncol(xtrain))
setwd("../")
setwd("../")
print(paste("no cols bf= ", ncol(xtest)))
testbound <- cbind(xtest, ytest)
print(paste("no cols after test= ", ncol(testbound)))
trainbound <- cbind(xtrain, ytrain)
print(paste("no cols after train= ", ncol(trainbound)))
print(paste("num rows test =", nrow(testbound), "num rows train= ", nrow(trainbound)))
traintestbound <- rbind(testbound, trainbound)
print(nrow(traintestbound))
return(traintestbound)
}
getMeanandMedianCols <- function()
{
#read in features table. get indices  of tiles containing strings mean
# and meadian only!!!
setwd(main_data_dir)
features <-read.table(features)
count <-nrow(features)
features = features[,2]
for( i in 1:count)
{
if(grepl("mean()", as.character(features[i])) | grepl("std()",as.character(features[i]))
| grepl("Mean",as.character(features[i]))  | grepl("meanFreq()",as.character(features[i])))
{
headerslist <- append(headerslist, as.character(features[i]))
headersindices <- append(headersindices, i)
}
}
setwd("../")
}
removeUnusedCols <- function(traintestbound)
{
traintestbound <- traintestbound[, headersindices]
print(nrow(headersindices))
print(ncol(traintestbound))
return(traintestbound)
}
Main <- function()
{
traintestbound <- initDataset()
getMeanandMedianCols()
removedcolsframe <- removeUnusedCols(traintestbound)
print(head(removecolsframe, n = 10))
}
